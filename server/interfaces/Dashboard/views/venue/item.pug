extends ../index

mixin field(name)
  .form-group
    label.col-sm-2.control-label(for='inputEmail3')= name
    .col-sm-10
      input#inputEmail3.form-control(type='text', placeholder=name, name=name, value= venue[name])

block content
  form.form-horizontal
    +field('id')
    +field('name')
    +field('city')
    +field('state')
    +field('street')
    +field('postalCode')
    +field('googleMapsUrl')
    +field('created')
    +field('href')
    .form-group
      .col-sm-offset-2.col-sm-10
        button.btn.btn-primary(type='submit') Save
        button.btn.btn-default(type='cancel') Cancel
